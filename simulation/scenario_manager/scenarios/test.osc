import osc.example
import osc.helpers
import osc.ros

scenario collision:
    do serial:
	action_call("/scenario_manager/collision_action", "simlan_custom_msg.action.Collision", "{\"angle\": 45}")

	action_call("/scenario_manager/collision_action", "simlan_custom_msg.action.Collision", "{\"angle\": 90}")

	action_call("/scenario_manager/collision_action", "simlan_custom_msg.action.Collision", "{\"angle\": 135}")

	action_call("/scenario_manager/collision_action", "simlan_custom_msg.action.Collision", "{\"angle\": 180}")
